{
  "plugins": [
    "prettier",
    "unused-imports"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": true
  },
  "rules": {
    "no-unused-vars": 2,
    "unused-imports/no-unused-imports": 2,
    "unused-imports/no-unused-vars": 2,
    "semi": [
      2,
      "never"
    ],
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": {
          "multiline": true,
          "minItems": 2
        },
        "ObjectPattern": {
          "multiline": true,
          "minItems": 2
        },
        "ImportDeclaration": {
          "multiline": true,
          "minItems": 2
        },
        "ExportDeclaration": {
          "multiline": true,
          "minItems": 2
        }
      }
    ]
  }
}