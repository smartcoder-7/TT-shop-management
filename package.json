  "scripts": {
    "set:env": "node ./scripts/set-env.js",
    "set:heroku-config": "node ./scripts/set-heroku-config.js",
    "build": "webpack",
    "watch": "yarn run set:env && webpack-dev-server",
    "start:local": "yarn run set:env && nodemon ./server/index.js -w ./server -w ./shared",
    "watch:dev": "NODE_ENV=development yarn run watch",
    "start:dev": "NODE_ENV=development yarn run start:local",
    "watch:dev:offline": "NODE_ENV=offline yarn run watch",
    "start:dev:offline": "NODE_ENV=offline yarn start:local",
    "start": "NODE_ENV=production node ./server/index.js",
    "deploy:staging": "NODE_ENV=staging yarn run set:heroku-config && ./scripts/deploy-staging.sh",
    "deploy:production": "NODE_ENV=production yarn run set:heroku-config && ./scripts/deploy-production.sh",
    "deploy:all": "yarn run deploy:staging && yarn run deploy:production"
  },
